rdf:Property a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:maxCount 1 ;
            sh:order 1 ;
            sh:path rdf:type ],
        [ sh:order 0 ;
            sh:path rhea:id ] ;
    sh:targetClass rdf:Property .

rdfs:Class a sh:NodeShape ;
