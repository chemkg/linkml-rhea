rhea:ReactivePart a sh:NodeShape ;
    sh:closed true ;
    sh:description "A reactive part in a generic compound, with a defined chemical structure. A subclass of a given ChEBI chemical entity." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:maxCount 1 ;
            sh:order 7 ;
            sh:path rhea:htmlName ],
        [ sh:order 0 ;
            sh:path rhea:id ],
        [ sh:maxCount 1 ;
            sh:order 1 ;
            sh:path rdf:type ],
        [ sh:maxCount 1 ;
            sh:order 3 ;
            sh:path rhea:charge ],
        [ sh:maxCount 1 ;
            sh:order 5 ;
            sh:path rhea:chebi ],
        [ sh:maxCount 1 ;
            sh:order 8 ;
            sh:path rhea:position ],
        [ sh:maxCount 1 ;
            sh:order 2 ;
            sh:path rhea:name ],
        [ sh:maxCount 1 ;
            sh:order 6 ;
            sh:path rhea:formula ],
        [ sh:class rdfs:Class ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 4 ;
            sh:path rdfs:subClassOf ] ;
    sh:targetClass rhea:ReactivePart .

