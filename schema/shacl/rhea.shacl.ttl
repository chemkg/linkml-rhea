@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rhea: <http://rdf.rhea-db.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

rhea:BidirectionalReaction a sh:NodeShape ;
    sh:closed true ;
    sh:description "A chemical reaction which can happen in one direction or the other, depending on the physiological conditions." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:maxCount 1 ;
            sh:order 14 ;
            sh:path rhea:bidirectionalReaction ],
        [ sh:order 8 ;
            sh:path rhea:substratesOrProducts ],
        [ sh:maxCount 1 ;
            sh:order 11 ;
            sh:path rhea:htmlEquation ],
        [ sh:order 0 ;
            sh:path rhea:id ],
        [ sh:maxCount 1 ;
            sh:order 10 ;
            sh:path rhea:isChemicallyBalanced ],
        [ sh:maxCount 1 ;
            sh:order 13 ;
            sh:path rdfs:comment ],
        [ sh:order 15 ;
            sh:path rhea:directionalReaction ],
        [ sh:in ( rhea:Approved rhea:Obsolete rhea:Preliminary ) ;
            sh:maxCount 1 ;
            sh:order 6 ;
            sh:path rhea:status ],
        [ sh:maxCount 1 ;
            sh:order 9 ;
            sh:path rdf:type ],
        [ sh:order 16 ;
            sh:path rhea:side ],
        [ sh:maxCount 1 ;
            sh:order 3 ;
            sh:path rdfs:label ],
        [ sh:maxCount 1 ;
            sh:order 5 ;
            sh:path rhea:equation ],
        [ sh:description "EC is rarely/never specified for bidirectional" ;
            sh:order 17 ;
            sh:path rhea:ec ],
        [ sh:maxCount 1 ;
            sh:order 7 ;
            sh:path rhea:isTransport ],
        [ sh:order 1 ;
            sh:path rdfs:seeAlso ],
        [ sh:maxCount 1 ;
            sh:order 4 ;
            sh:path rhea:accession ],
        [ sh:class rdfs:Class ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 12 ;
            sh:path rdfs:subClassOf ],
        [ sh:order 2 ;
            sh:path rhea:citation ] ;
    sh:targetClass rhea:BidirectionalReaction .

rhea:Compound a sh:NodeShape ;
    sh:closed true ;
    sh:description "A chemical compound" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:order 0 ;
            sh:path rhea:id ],
        [ sh:maxCount 1 ;
            sh:order 2 ;
            sh:path rdfs:comment ],
        [ sh:maxCount 1 ;
            sh:order 3 ;
            sh:path rdfs:label ],
        [ sh:maxCount 1 ;
            sh:order 1 ;
            sh:path rdf:type ] ;
    sh:targetClass rhea:Compound .

rhea:DirectionalReaction a sh:NodeShape ;
    sh:closed true ;
    sh:description "A chemical reaction, with the direction specified by substrates and products." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:maxCount 1 ;
            sh:order 5 ;
            sh:path rhea:htmlEquation ],
        [ sh:in ( rhea:Approved rhea:Obsolete rhea:Preliminary ) ;
            sh:maxCount 1 ;
            sh:order 9 ;
            sh:path rhea:status ],
        [ sh:order 13 ;
            sh:path rdfs:seeAlso ],
        [ sh:maxCount 1 ;
            sh:order 2 ;
            sh:path rdf:type ],
        [ sh:maxCount 1 ;
            sh:order 3 ;
            sh:path rhea:isChemicallyBalanced ],
        [ sh:maxCount 1 ;
            sh:order 1 ;
            sh:path rhea:isTransport ],
        [ sh:maxCount 1 ;
            sh:order 4 ;
            sh:path rhea:substrates ],
        [ sh:order 7 ;
            sh:path rhea:citation ],
        [ sh:order 16 ;
            sh:path rhea:directionalReaction ],
        [ sh:maxCount 1 ;
            sh:order 15 ;
            sh:path rhea:bidirectionalReaction ],
        [ sh:order 0 ;
            sh:path rhea:id ],
        [ sh:maxCount 1 ;
            sh:order 6 ;
            sh:path rhea:accession ],
        [ sh:maxCount 1 ;
            sh:order 11 ;
            sh:path rdfs:label ],
        [ sh:maxCount 1 ;
            sh:order 8 ;
            sh:path rhea:equation ],
        [ sh:class rdfs:Class ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 12 ;
            sh:path rdfs:subClassOf ],
        [ sh:order 17 ;
            sh:path rhea:side ],
        [ sh:maxCount 1 ;
            sh:order 14 ;
            sh:path rdfs:comment ],
        [ sh:description "EC is rarely/never specified for directional" ;
            sh:order 18 ;
            sh:path rhea:ec ],
        [ sh:maxCount 1 ;
            sh:order 10 ;
            sh:path rhea:products ] ;
    sh:targetClass rhea:DirectionalReaction .

rhea:GenericCompound a sh:NodeShape ;
    sh:closed true ;
    sh:description "A chemical compound whose complete chemical structure is not described. Have one or or more reactive part(s) with chemical structure(s)." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:maxCount 1 ;
            sh:order 3 ;
            sh:path rdfs:comment ],
        [ sh:maxCount 1 ;
            sh:order 2 ;
            sh:path rdf:type ],
        [ sh:order 0 ;
            sh:path rhea:id ],
        [ sh:maxCount 1 ;
            sh:order 1 ;
            sh:path rdfs:label ] ;
    sh:targetClass rhea:GenericCompound .

rhea:GenericPolynucleotide a sh:NodeShape ;
    sh:closed true ;
    sh:description "A generic compound that is a polynucleotide. Have one or or more reactive part(s) with chemical structure(s)." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:maxCount 1 ;
            sh:order 2 ;
            sh:path rdf:type ],
        [ sh:maxCount 1 ;
            sh:order 4 ;
            sh:path rhea:accession ],
        [ sh:maxCount 1 ;
            sh:order 5 ;
            sh:path rhea:htmlName ],
        [ sh:order 3 ;
            sh:path rhea:reactivePart ],
        [ sh:maxCount 1 ;
            sh:order 1 ;
            sh:path rhea:name ],
        [ sh:maxCount 1 ;
            sh:order 6 ;
            sh:path rdfs:label ],
        [ sh:order 0 ;
            sh:path rhea:id ],
        [ sh:maxCount 1 ;
            sh:order 7 ;
            sh:path rdfs:comment ] ;
    sh:targetClass rhea:GenericPolynucleotide .

rhea:GenericPolypeptide a sh:NodeShape ;
    sh:closed true ;
    sh:description "A generic compound that is a polypeptide. Have one or or more reactive part(s) with chemical structure(s)." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:maxCount 1 ;
            sh:order 7 ;
            sh:path rdfs:comment ],
        [ sh:maxCount 1 ;
            sh:order 6 ;
            sh:path rdfs:label ],
        [ sh:maxCount 1 ;
            sh:order 5 ;
            sh:path rhea:htmlName ],
        [ sh:maxCount 1 ;
            sh:order 4 ;
            sh:path rhea:accession ],
        [ sh:order 1 ;
            sh:path rhea:reactivePart ],
        [ sh:maxCount 1 ;
            sh:order 3 ;
            sh:path rdf:type ],
        [ sh:order 0 ;
            sh:path rhea:id ],
        [ sh:maxCount 1 ;
            sh:order 2 ;
            sh:path rhea:name ] ;
    sh:targetClass rhea:GenericPolypeptide .

rhea:Location a sh:NodeShape ;
    sh:closed true ;
    sh:description "A location for a reaction participant." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:order 0 ;
            sh:path rhea:id ],
        [ sh:maxCount 1 ;
            sh:order 1 ;
            sh:path rdf:type ] ;
    sh:targetClass rhea:Location .

rhea:Polymer a sh:NodeShape ;
    sh:closed true ;
    sh:description "A chemical compound that is a polymer. Described by a polymerization index, and an underlying polymer from ChEBI." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:maxCount 1 ;
            sh:order 2 ;
            sh:path rhea:charge ],
        [ sh:maxCount 1 ;
            sh:order 6 ;
            sh:path rhea:htmlName ],
        [ sh:maxCount 1 ;
            sh:order 4 ;
            sh:path rhea:formula ],
        [ sh:maxCount 1 ;
            sh:order 10 ;
            sh:path rdfs:label ],
        [ sh:order 0 ;
            sh:path rhea:id ],
        [ sh:maxCount 1 ;
            sh:order 3 ;
            sh:path rhea:underlyingChebi ],
        [ sh:maxCount 1 ;
            sh:order 7 ;
            sh:path rhea:name ],
        [ sh:in ( "n-2" "n+1" "n-3" "n-1" "n" "n-5" "n-4" "n+2" ) ;
            sh:maxCount 1 ;
            sh:order 8 ;
            sh:path rhea:polymerizationIndex ],
        [ sh:maxCount 1 ;
            sh:order 5 ;
            sh:path rhea:accession ],
        [ sh:maxCount 1 ;
            sh:order 1 ;
            sh:path rdf:type ],
        [ sh:maxCount 1 ;
            sh:order 9 ;
            sh:path rdfs:comment ] ;
    sh:targetClass rhea:Polymer .

rhea:Reaction a sh:NodeShape ;
    sh:closed true ;
    sh:description "A chemical reaction, with unspecified direction." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:maxCount 1 ;
            sh:order 13 ;
            sh:path rdfs:comment ],
        [ sh:order 0 ;
            sh:path rhea:id ],
        [ sh:maxCount 1 ;
            sh:order 2 ;
            sh:path rhea:accession ],
        [ sh:maxCount 1 ;
            sh:order 9 ;
            sh:path rhea:htmlEquation ],
        [ sh:maxCount 1 ;
            sh:order 7 ;
            sh:path rdf:type ],
        [ sh:order 6 ;
            sh:path rhea:citation ],
        [ sh:maxCount 1 ;
            sh:order 3 ;
            sh:path rhea:isTransport ],
        [ sh:order 11 ;
            sh:path rhea:side ],
        [ sh:class rdfs:Class ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 14 ;
            sh:path rdfs:subClassOf ],
        [ sh:maxCount 1 ;
            sh:order 12 ;
            sh:path rdfs:label ],
        [ sh:order 8 ;
            sh:path rhea:directionalReaction ],
        [ sh:description "EC is always specified for unspecified direction" ;
            sh:order 15 ;
            sh:path rhea:ec ],
        [ sh:order 16 ;
            sh:path rdfs:seeAlso ],
        [ sh:in ( rhea:Approved rhea:Obsolete rhea:Preliminary ) ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path rhea:status ],
        [ sh:maxCount 1 ;
            sh:order 10 ;
            sh:path rhea:equation ],
        [ sh:maxCount 1 ;
            sh:order 5 ;
            sh:path rhea:isChemicallyBalanced ],
        [ sh:maxCount 1 ;
            sh:order 4 ;
            sh:path rhea:bidirectionalReaction ] ;
    sh:targetClass rhea:Reaction .

rhea:ReactionParticipant a sh:NodeShape ;
    sh:closed true ;
    sh:description "A reaction participant. Contained in a reaction side." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:maxCount 1 ;
            sh:order 3 ;
            sh:path rdf:type ],
        [ sh:in ( rhea:Out rhea:In ) ;
            sh:maxCount 1 ;
            sh:order 4 ;
            sh:path rhea:location ],
        [ sh:order 0 ;
            sh:path rhea:id ],
        [ sh:maxCount 1 ;
            sh:order 2 ;
            sh:path rhea:compound ],
        [ sh:class rdfs:Class ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 1 ;
            sh:path rdfs:subClassOf ] ;
    sh:targetClass rhea:ReactionParticipant .

